{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Serve Jekyll Site",
      "type": "shell",
      "command": "bundle exec jekyll serve --livereload --baseurl=''",
      "isBackground": true,
      "problemMatcher": {
        "owner": "jekyll",
        "pattern": [
          {
            "regexp": ".*"
          }
        ],
        "background": {
          "activeOnStart": true,
          "beginsPattern": ".*Server running.*",
          "endsPattern": ".*Server running.*"
        }
      },
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
